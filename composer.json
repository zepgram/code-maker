{
    "name": "zepgram/code-maker",
    "type": "magento2-component",
    "description": "Helps you to create code on the fly for magento2",
    "keywords": [
        "generator",
        "code generator",
        "installer",
        "scaffolding",
        "scaffold"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Calef",
            "email": "zepgram@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "symfony/console": "^2.8.0 || ^3.0.0 || ^4.0.0"
    },
    "extra": {
        "chmod": [
            {
                "mask": "0755",
                "path": "bin/zepgram"
            }
        ],
        "map": [
            [
                "src/dist/bin/zepgram",
                "bin/zepgram"
            ]
        ]
    },
    "autoload": {
        "psr-4": {
            "Zepgram\\CodeMaker\\": "src"
        }
    }
}
