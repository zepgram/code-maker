{
    "name": "zepgram/code-maker",
    "description": "Helps you to create code on the fly for magento2",
    "keywords": ["generator", "code generator", "installer", "scaffolding", "scaffold"],
    "type": "magento2-component",
    "authors":[
        {
            "name": "Benjamin Calef",
            "email": "benjamin.calef@caudalie.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "symfony/console": "^2.6|^4.0",
        "ext-simplexml": "*",
        "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "Zepgram\\CodeMaker\\": "src"
        }
    },
    "extra": {
        "map": [
            [
                "src/dist/bin/zepgram",
                "bin/zepgram"
            ]
        ],
        "chmod": [
            {
                "mask": "0755",
                "path": "bin/zepgram"
            }
        ]
    }
}
